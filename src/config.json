{
    "task": "Data Imputation",
    "dataset": {
        "name": "buy",
        "target_column": "manufacturer"
    },
    "model": "GPT Small",
    "column_length_limit": 200,
    "repeat": 5,
    "sampling": {
        "method": "Group Sampling",
        "number_of_samples": 50,
        "m": 10,
        "n": 10
    },
    "examples": {
        "method": "similarity",
        "number_of_examples": 3,
        "random_seed": 42,
        "rows": [
            1,
            2,
            3
        ]
    },
    "ner": {
        "method": null,
        "number_of_examples": 1,
        "atomicity_threshold": 0.7,
        "entity_detection_threshold": 0.5
    },
    "dependency_finder": {
        "method": "LCS",
        "number_of_rules": -1,
        "inner_threshold": 0.9,
        "outer_threshold": 0.9
    },
    "dataset_partition": {
        "train_ratio": 0.8,
        "number_of_test_rows": 3,
        "random_seed": 42
    },
    "evaluate": {
        "methods": [
            "exact_match",
            "substr_match"
        ]
    },
    "result_path": "res.xlsx",
    "output_path": "out.txt"
}